{
    "name": "GrepTest",
    "tool-version": "0.1",
    "author": "Axel Bonnet",
    "description": "Test pipelines that search text in a file (without docker, local).<br />",
    "command-line": "sleep [SLEEPTIME]; echo 'flag [FLAG]'; grep [TEXT] [INPUT] > [OUTPUT]",
    "schema-version": "0.5",
    "inputs": [{
        "id": "text",
        "name": "Text",
        "type": "String",
        "description": "Text to search",
        "value-key": "[TEXT]"
      },
      {
        "id": "file",
        "name": "File",
        "type": "File",
        "description": "File to search in",
        "value-key": "[INPUT]"
      },
      {
        "id": "sleepTime",
        "name": "sleepTime",
        "type": "Number",
        "description": "Additional param to test boolean",
        "value-key": "[SLEEPTIME]"
      },
      {
        "id": "flag",
        "name": "Flag",
        "type": "Flag",
        "optional": true,
        "command-line-flag":"--testflag",
        "value-key": "[FLAG]"
      }
    ],
    "output-files": [{
      "id": "output",
      "name": "output file",
      "path-template": "output_[TEXT]_[SLEEPTIME].txt",
      "value-key": "[OUTPUT]"
    }]
  }
  